{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs" ,
    "strict": true ,
    "noUnusedLocals": false,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "types": ["node"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true 
  }
}
